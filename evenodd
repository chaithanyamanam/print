#include <stdio.h>
int main()
{
    int n,i,a[20];
    printf("enter array size");
    scanf("%d",&n);
    printf("enter elements");
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
   
    for(i=0;i<n;i++)
    {
        if(a[i]%2==1)
        {
            printf("%d",a[i]);
        }
    }
      printf(" ");
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
         printf("%d",a[i]);
        }
    }
    return 0;
}
